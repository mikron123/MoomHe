{
  "appId": "com.kalromsystems.moomhe",
  "appName": "MoomHe",
  "webDir": "dist",
  "server": {
    "androidScheme": "https"
  },
  "android": {
    "allowMixedContent": true,
    "captureInput": true,
    "webContentsDebuggingEnabled": true
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#1a1a2e",
      "showSpinner": false,
      "androidScaleType": "CENTER_CROP"
    }
  }
}
